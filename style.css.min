body{font:1.2em/1.62em sans-serif;margin:1em auto;max-width:48em;padding:.62em;line-height:1.5}h1,h2,h3{line-height:1.2em}#content{text-align:justify}code,pre{background:#eee;max-width:100%}code{padding:2px;vertical-align:text-bottom}pre{padding:1em;border-left:2px solid #096;overflow:auto;word-wrap:normal;white-space:pre}@media print{body{max-width:none}}img{max-width:100%;height:auto;display:block;margin-left:auto;margin-right:auto}.nav{margin-bottom:20px}.nav .navbar-list{padding:0;text-align:center}.nav .navbar-list li{display:inline-block;vertical-align:top;font-size:1.62em;margin-right:6px;padding:10px 20px}
